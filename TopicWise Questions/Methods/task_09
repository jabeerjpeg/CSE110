import java.util.Scanner;
public class task9 {
    public static void main(String []args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int x=sumDigits(n);
        System.out.println(x);

    }

    public static int sumDigits(int n) {
        if(n%10==n) {
            return n;
        }
        else {
            return n%10 +sumDigits(n/10);
        }

    }
}

