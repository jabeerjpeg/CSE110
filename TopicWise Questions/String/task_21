public class task23_prev {
    public static void main(String[] args) {
        String s1 = "niggox";
        String s1_modified = "";
        for(int i = 0; i < s1.length(); i++){
            char ch = s1.charAt(i);
            if(ch !='o'){
                s1_modified += ch;
            }else{
                ch += 9;
                s1_modified += ch;
            }
        }
        System.out.println(s1_modified);
    }
}
