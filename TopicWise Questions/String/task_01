import java.util.Scanner;
public class task1 {
public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
String s1 = sc.nextLine();
String s2 = "";
for (int i = 0; i < s1.length(); i++) {
char temp = s1.charAt(i);
if (temp >= 'a' && temp <= 'z') {
temp = (char) (temp - 32);
}
s2 += temp;
}
System.out.println(s2);
sc.close();
}
}
