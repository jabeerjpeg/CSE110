
import java.util.Scanner;
public class task3 {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    String s1 = sc.nextLine();
    char s2 = sc.next().charAt(0);
    for (int i = 0; i < s1.length(); i++) {
      if( s1.charAt(i) == s2){
        System.out.println("");
        continue;
      }
      System.out.print(s1.charAt(i));
    }
  }
}
