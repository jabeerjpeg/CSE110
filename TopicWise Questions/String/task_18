public class task20_prev {
    public static void main(String[] args) {
        String s1= "Xavier";
        String s2= "X";
        if(s2.length()<2){
            return;
        }
            if(s1.charAt(0)==s2.charAt(0) || s1.charAt(1)==s2.charAt(1)){
                System.out.print("same");
            }

