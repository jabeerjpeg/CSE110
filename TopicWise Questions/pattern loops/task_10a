import java.util.Scanner;

public class pattern4_7a {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int row = sc.nextInt();   //row =8
        for(int rc=1; rc<=row;rc++){
            for(int cc=1; cc<=rc;cc++){
                System.out.print("# ");
            }
            System.out.println();
        }
    }
}

