public class task1a {
  public static void main(String[] args) {
    int num1= 100;
    int num2= 23;
    int num3= -4;
    
    int largest = num1; // let num1 be the largest
    
    if (num2 > largest) {
      largest = num2; 
    }
    if (num3 > largest) {
      largest = num3; 
    }
    
    System.out.println("Largest number: " + largest);
    
    
  }
}
