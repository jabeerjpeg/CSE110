public class task6 {
    public static void main(String[] args) {
        int x = -3;
        int output;
        
        if (x < 0) {
            output = 2 * x; 
        } else if (x > 0 && x < 2) {
            output = x + 1;
        } else if (x >= 2 && x < 5) {
            output = x * x - 1; 
        } else { // x >= 5
            output = 3 * x * x + 2; 
        }

        System.out.println("Output: " + output);

    }
}
