public class task7{
    public static void main(String[] args) {
      int n =6;
      int count=0;
       System.out.println("Divisors of " + n + ":");
      for (int i=1; i<=n; i++) {
          if( n%i ==0) {
          System.out.println (i);
          count++;
        }
      }
      System.out.println("Total Divisors: "+ count);
        
    }
}
