public class task8{
    public static void main(String[] args) {
    int n = 5; 
    int sum = 0;
    System.out.println("The odd numbers are:");
    for (int i = 0; i < n; i++) {
      int odd = 2 * i + 1; 
      System.out.println(odd); 
      sum += odd; 
    }
   
    System.out.println("The sum of the first " + n + " odd natural numbers is " + sum);
  }
}
