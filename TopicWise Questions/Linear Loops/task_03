public class task3{
    public static void main(String[] args) {
        int limit = 600;
        int sum = 0;

        for (int i = 1; i <= limit; i++) {
            
            if (i % 7 == 0 && i % 9 == 0) {
                sum += i; 
            }
        }

       
        System.out.println("The sum of all numbers that are multiples of both 7 and 9 up to 600 is: " + sum);
    }
}

