import java.util.Scanner;
public class no2{
  public static void main (String [] args){
    Scanner sc= new Scanner (System.in);
    int x;
    while(true){
      System.out.println ("Enter Number: ");
      x=sc.nextInt();
      if(x<0){
        break;
      }
      System.out.println (x*x);
    }
  }
} 
